        "eightpoints/guzzle-bundle": "^7.3",
        "friendsofsymfony/user-bundle": "^2.1",
        "garlic/auth": "^1.1",
        "garlic/bus": "^1.0",
        "garlic/user": "dev-master",
        "hwi/oauth-bundle": "^0.6.1",
        "incenteev/composer-parameter-handler": "^2.1",
        "lcobucci/jwt": "^3.2",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nelmio/api-doc-bundle": "^3.1",
        "nelmio/cors-bundle": "^1.5",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.8",
        "predis/predis": "^1.1",
        "qandidate/symfony-json-request-transformer": "^1.3",
        "rollbar/rollbar": "^1.4",
        "scheb/two-factor-bundle": "^2.14",
        "sensio/framework-extra-bundle": "^5.1",
        "snc/redis-bundle": "^2.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/ldap": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-apcu": "^1.6|^4.0",
        "symfony/security": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C08TKSXXRJ9JNCW2HWP6S1CH",
            "allow-contrib": false
        }
    }
}
